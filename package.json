{
  "name": "expo-bare-features",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "set CURRENT_ENV=dev&& npx react-native start",
    "dev": "set CURRENT_ENV=dev&& npx expo start --clear",
    "prod": "set CURRENT_ENV=prod&& npx expo start --clear",
    "api": "json-server --watch ./src/database/db.json -p 3333",
    "exposeApi": "npx ngrok http 3333",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:development": "eas build --profile development",
    "build:production": "eas build --profile production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-firebase/app": "^15.7.0",
    "@react-native-firebase/auth": "^15.7.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "axios": "^0.27.2",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "expo": "^46.0.9",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "native-base": "^3.4.13",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.7",
    "react-navigation": "^4.4.4",
    "react-query": "^3.39.2",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/axios": "^0.14.0",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-navigation": "^3.4.0",
    "@types/react-query": "^1.2.9",
    "json-server": "^0.17.0"
  },
  "private": true
}
